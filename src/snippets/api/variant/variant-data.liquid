{% endcomment %}

<script type="application/json" data-from-shopify="variant">
{
  "data": [
    {%- for variant in variants -%}
      {%- include 'variant-object',
        _product: _product,
        variant: variant -%}{%- unless forloop.last -%},{%- endunless -%}
    {%- endfor -%}
  ]
}
</script>
